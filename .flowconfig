[ignore]
/node_modules/*
/test/*

[libs]
internals/flow/

[options]
module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.json
module.file_ext=.css
module.file_ext=.scss
module.file_ext=.less
module.name_mapper.extension='css' -> 'empty/object'
module.name_mapper.extension='scss' -> 'empty/object'
module.name_mapper.extension='less' -> 'empty/object'

esproposal.decorators=ignore


;自定義路徑
module.name_mapper='^@static' ->'<PROJECT_ROOT>/static'
module.name_mapper='^@lang' ->'<PROJECT_ROOT>/src/resources/lang'
module.name_mapper='^@assets' ->'<PROJECT_ROOT>/src/resources/assets'
module.name_mapper='^@themes' ->'<PROJECT_ROOT>/src/resources/themes'
module.name_mapper='^@components' ->'<PROJECT_ROOT>/src/resources/components'
module.name_mapper='^@utils' ->'<PROJECT_ROOT>/src/utils'
module.name_mapper='^@library' ->'<PROJECT_ROOT>/src/library'
module.name_mapper='^@services' ->'<PROJECT_ROOT>/src/services'
module.name_mapper='^@pages' ->'<PROJECT_ROOT>/src/pages'
module.name_mapper='^@config' ->'<PROJECT_ROOT>/src/config'
module.name_mapper='^@middleware' ->'<PROJECT_ROOT>/src/middleware'

[version]
^0.86.0
